<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/relLayoutEditProfile1">


            <include layout="@layout/snippet_top_editprofiletoolbar"/>

        </RelativeLayout>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_below="@id/relLayoutEditProfile1"
            android:id="@+id/relLayoutEditProfile2"
            android:background="@color/white">

            <include layout="@layout/snipped_center_editprofile"/>


        </RelativeLayout>


    </RelativeLayout>

</android.support.constraint.ConstraintLayout>